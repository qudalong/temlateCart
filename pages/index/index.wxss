swiper {
  height: 280rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.banner {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 20rpx;
  background: #fff;
}

.container {
  background: #fff;
  padding: 0 20rpx;
}

.banner image {
  height: 210rpx;
  width: 100%;
  border-radius: 20rpx;
  margin: 20rpx 0;
}



.filter-wrap {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  padding: 10rpx 0 20rpx;
}

.picker-change {
  min-width: 100rpx;
  max-width: 36%;
}

.search {
  width: 60%;
}

.list-wrap .item {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
  border-top: 1px solid #e5e5e5;
}

.list-wrap .item .name {
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list-wrap .details {
  flex: 1;
  width: 100px;
}

.list-wrap .item .adress {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-right: 10rpx;
}

.list-wrap .item .distance, .list-wrap .item .adress {
  font-size: 26rpx;
  color: #666;
  padding-top: 14rpx;
}

.position {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.icon-position {
  width: 26rpx;
  height: 31rpx;
}

.search input {
  border: 1px solid #ccc;
  border-radius: 42rpx;
  padding: 10rpx 10rpx 10rpx 16rpx;
  background: url('data:image/png;base64,') no-repeat 96% center;
  background-size: 40rpx 40rpx;
  padding-right: 56rpx;
}

